{
    "package": {
        "name": "vlingo-directory",
        "repo": "vlingo-platform-java",
        "subject": "vlingo",
        "desc": "Service registration and discovery for the vlingo/platform.",
        "website_url": "https://github.com/vlingo/vlingo-directory",
        "issue_tracker_url": "https://github.com/vlingo/vlingo-directory/issues",
        "vcs_url": "https://github.com/vlingo/vlingo-directory.git",
        "licenses": ["MPL-2.0"],
        "public_download_numbers": true,
        "public_stats": true
    },

    "version": {
        "name": "1.2.3",
        "gpgSign": true
    },

    "files": [
        {
            "includePattern": "target/(.*).jar",
            "uploadPattern": "/io/vlingo/vlingo-directory/1.2.3/$1.jar",
            "matrixParams": {
                "override": 1
            }
        },
        {
            "includePattern": "pom.xml",
            "uploadPattern": "/io/vlingo/vlingo-directory/1.2.3/vlingo-directory-1.2.3.pom",
            "matrixParams": {
                "override": 1
            }
        }
    ],

    "publish": true
}
